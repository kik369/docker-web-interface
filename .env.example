# Flask Environment Variables
FLASK_ENV=development                    # Options: development, production
FLASK_APP=docker_monitor.py             # The main application file

# Logging Configuration
LOG_LEVEL=INFO                          # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FORMAT=json                         # Options: json, text
PYTHONPATH=/app                         # Python path for the application

# Frontend Configuration
REACT_APP_API_URL=http://localhost:5000 # Backend API URL
REACT_APP_LOG_LEVEL=info               # Frontend logging level
REACT_APP_SEND_LOGS_TO_BACKEND=true    # Whether to send frontend logs to backend

# Add your custom environment variables below
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname
# SECRET_KEY=your_secret_key_here
# API_KEY=your_api_key_here
